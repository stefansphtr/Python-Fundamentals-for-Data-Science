# Create calculate_insurance_cost
def calculate_insurance_cost(name, age, sex, bmi, num_of_children, smoker):
    estimated_cost = 250 * age - 128 * sex + 370 * bmi + 425 * num_of_children + 2400 * smoker - 12500
    print(f"The estimated cost for {name} is: ${estimated_cost}.")
    return estimated_cost 
  
# Calculate insurance cost for Maria
maria_insurance_cost = calculate_insurance_cost("Maria", 28, 0, 26.2, 3, 0)  

# Calculate insurance cost for Omar
omar_insurance_cost = calculate_insurance_cost(age=35, sex=1, smoker=1, bmi=22.2, name="Omar", num_of_children=0)